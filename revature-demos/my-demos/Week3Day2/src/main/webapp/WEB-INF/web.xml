<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Week3Day2</display-name>
  
  <servlet>
  	<servlet-name>FirstServlet</servlet-name>
  	<servlet-class>com.revature.servlets.OurFirstServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>FirstServlet</servlet-name>
  	<url-pattern>/first</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>com.revature.servlets.LoginServlet</servlet-class>
  	<init-param>
  		<param-name>someVar</param-name>
  		<param-value>hello</param-value>
  	</init-param>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <context-param>
  	<param-name>shared</param-name>
  	<param-value>share</param-value>
  </context-param>

  
</web-app>